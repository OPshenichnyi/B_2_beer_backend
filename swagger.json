{
  "openapi": "3.0.0",
  "info": {
    "title": "Swagger API",
    "version": "1.0.0",
    "description": "API documentation"
  },
  "paths": {
    "/api/users": {
      "get": {
        "summary": "Retrieve a list of users",
        "responses": {
          "200": {
            "description": "A list of users.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
