{
  "openapi": "3.0.0",
  "info": {
    "title": "B_2_Beer web aplication",
    "description": "The API for the B-2-Beer backend",
    "version": "1.0.0"
  },
  "-servers": {
    "-url": "https://b-2-beer-backend.vercel.app/",
    "description": "Dev server"
  },
  "paths": {
    "/register": {
      "post": {
        "summary": "Register a new users.",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  },
                  "clientType": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          }
        }
      }
    }
  }
}